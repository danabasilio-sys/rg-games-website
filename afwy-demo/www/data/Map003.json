{"id":1,"name":"EV001","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":250,"indent":0,"parameters":[{"name":"manWalk","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",1,0,2]},{"code":401,"indent":0,"parameters":["\\C[17]???:\\C"]},{"code":401,"indent":0,"parameters":["MC!"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["I look up from my phone and turn my head to see a man "]},{"code":401,"indent":0,"parameters":["walking up to the bench that I was sitting on. He "]},{"code":401,"indent":0,"parameters":["smiled and waved at me."]},{"code":101,"indent":0,"parameters":["",3,0,2]},{"code":401,"indent":0,"parameters":["\\C[24]MC:\\C"]},{"code":401,"indent":0,"parameters":["Hey, Dylan! Did you get here safely?"]},{"code":251,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["Dylan",1,0,2]},{"code":401,"indent":0,"parameters":["\\C[17]Dylan:\\C"]},{"code":401,"indent":0,"parameters":["It was fine. Traffic was terrible around my area,"]},{"code":401,"indent":0,"parameters":["but I'm here now."]},{"code":101,"indent":0,"parameters":["Dylan",2,0,2]},{"code":401,"indent":0,"parameters":["\\C[17]Dylan:\\C"]},{"code":401,"indent":0,"parameters":["I hope I didn't keep you waiting for too long."]},{"code":101,"indent":0,"parameters":["Dylan",2,0,2]},{"code":401,"indent":0,"parameters":["Dylan asked me to watch him busk today. His equipment "]},{"code":401,"indent":0,"parameters":["is probably in his car. My eyes glance at his car "]},{"code":401,"indent":0,"parameters":["parked on the street behind him before looking at "]},{"code":401,"indent":0,"parameters":["him with a soft smile."]},{"code":101,"indent":0,"parameters":["Dylan",2,0,2]},{"code":401,"indent":0,"parameters":["\\C[24]MC:\\C"]},{"code":401,"indent":0,"parameters":["Not really, you're fine. \\.\\.\\.We could have met at the "]},{"code":401,"indent":0,"parameters":["location where you planned to busk instead of meeting "]},{"code":401,"indent":0,"parameters":["halfway."]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["He grins at me before rubbing the back of his head."]},{"code":101,"indent":0,"parameters":["Dylan",1,0,2]},{"code":401,"indent":0,"parameters":["\\C[17]Dylan:\\C"]},{"code":401,"indent":0,"parameters":["Actually, \\.\\.\\.I was gonna ask you where I should busk "]},{"code":401,"indent":0,"parameters":["today."]},{"code":101,"indent":0,"parameters":["Dylan",1,0,2]},{"code":401,"indent":0,"parameters":["\\C[24]MC:\\C"]},{"code":401,"indent":0,"parameters":["Huh? \\.\\.Me? \\.\\.Why?"]},{"code":101,"indent":0,"parameters":["Dylan",1,0,2]},{"code":401,"indent":0,"parameters":["\\C[17]Dylan:\\C"]},{"code":401,"indent":0,"parameters":["I need a change of pace instead of my usual spots. \\.\\.\\."]},{"code":401,"indent":0,"parameters":["I was hoping you could show me a place."]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["It does sound like a great idea to go somewhere new. It "]},{"code":401,"indent":0,"parameters":["would increase his chances of getting discovered if he "]},{"code":401,"indent":0,"parameters":["busks in other places. I do have two different spots in"]},{"code":401,"indent":0,"parameters":["mind."]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["One is a crowded place that multiple people pass by.\\.\\.\\. "]},{"code":401,"indent":0,"parameters":["While the other is a more secluded area but it's around "]},{"code":401,"indent":0,"parameters":["multiple expensive establishments."]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["Where should we go?"]},{"code":102,"indent":0,"parameters":[["Public place","Secluded place"],-1,-1,1,0]},{"code":402,"indent":0,"parameters":[0,"Public place"]},{"code":201,"indent":1,"parameters":[0,14,7,12,0,0]},{"code":0,"indent":1,"parameters":[]},{"code":402,"indent":0,"parameters":[1,"Secluded place"]},{"code":201,"indent":1,"parameters":[0,15,8,12,0,0]},{"code":0,"indent":1,"parameters":[]},{"code":404,"indent":0,"parameters":[]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true}],"x":6,"y":10}
